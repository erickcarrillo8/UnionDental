
    <%= form_for(@cita) do |f| %>
        <%@cita.errors.full_messages.each do |message| %>
            <div>
                <%= message %>
            </div>
            
        <%end%>
         


        <div class="row">
            <div class="input-field col s12 l10">
                <%= f.date_field :fecha_inicio,   class:"datepicker" %>
                <%= f.date_field :fecha_final, class:"datepicker" %>
            </div>   
        
        </div>
        
        
        
        
        
    
    
        
         <%= f.submit "Guardar", class:"btn waves-effect waves-light" %>
    <%end%>
    
   



