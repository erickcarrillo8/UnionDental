<!DOCTYPE html>
<html>
<head>
    <title>Uniondental</title>
    <!--Import Google Icon Font-->
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    
    <link href="http://www.yonkethecardepot.com/backup/assets/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="http://www.yonkethecardepot.com/backup/assets/css/style.css" type="text/css" rel="stylesheet" media="screen,projection">
   
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    
    <link href="http://www.yonkethecardepot.com/backup/assets/js/plugins/perfect-scrollbar/perfect-scrollbar.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="http://www.yonkethecardepot.com/backup/assets/js/plugins/jvectormap/jquery-jvectormap.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="http://www.yonkethecardepot.com/backup/assets/js/plugins/chartist-js/chartist.min.css" type="text/css" rel="stylesheet" media="screen,projection">

   <!-- Scripts -->   
   <!-- jQuery Library -->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/jquery-1.11.2.min.js"></script>    
</head>
<body>
     <!-- Start Page Loading -->
    <div id="loader-wrapper">
        <div id="loader"></div>        
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!-- End Page Loading -->

    <!-- //////////////////////////////////////////////////////////////////////////// -->

    <!-- START HEADER -->
    <header id="header" class="page-topbar">
        <!-- start header nav-->
        <div class="navbar-fixed">
            <nav class="cyan">
                <div class="nav-wrapper">
                    <h1 class="logo-wrapper"><a href="index.html" class="brand-logo darken-1"><img src="http://www.clinicadentalunion.com/wp-content/uploads/2014/05/logo.png" style="height: 50px;" alt="union dental  logo"></a> <span class="logo-text">Materialize</span></h1>
                    
                </div>
            </nav>
        </div>
        <!-- end header nav-->
    </header>
    <!-- END HEADER -->

    <!-- //////////////////////////////////////////////////////////////////////////// -->

    <!-- START MAIN -->
    <div id="main">
        <!-- START WRAPPER -->
        <div class="wrapper">

            <!-- START LEFT SIDEBAR NAV-->
            <aside id="left-sidebar-nav">
                <ul id="slide-out" class="side-nav fixed leftside-navigation">
                    <li class="user-details cyan darken-2">
                        <div class="row">
                            <div class="col col s4 m4 l4">
                                <img src="http://www.yonkethecardepot.com/backup/assets/images/avatar.jpg" alt="" class="circle responsive-img valign profile-image">
                            </div>
                            <div class="col col s8 m8 l8">
                                <ul id="profile-dropdown" class="dropdown-content">
                                        
                                    <li><%= link_to "Cerrar Sesion", destroy_dentist_session_path, method: :delete %> </li>
                                   
                                </ul>
                                <a class="btn-flat dropdown-button waves-effect waves-light white-text profile-btn" href="#" data-activates="profile-dropdown">Dr <%= current_dentist.name %><i class="material-icons right"><i class="material-icons">expand_more</i></i></i></a>
                                <p class="user-roal">Dentista</p>
                            </div>
                        </div>
                    </li>
                    <li class="bold">
                        <%= link_to root_path , class:"waves-effect waves-cyan" do %>
                        
                            <i class="material-icons">dashboard</i> Inicio
                        
                        <% end %>
                    </li>
                    <li class="bold">
                   
                    <%= link_to  patients_path , class:"waves-effect waves-cyan" do %> 
                        <i class="material-icons">supervisor_account</i> 
                        Pacientes
                    <% end %>
                    
                    </li>
                    <li class="bold">
                        <%=  link_to dentists_path , class:"waves-effect waves-cyan" do  %>
                            <i class="material-icons">people_outline</i></i> Dentistas
                        <% end %>
                    </li>
                    
                                     
                    <li class="li-hover"><div class="divider"></div></li>
                    
                  
                </ul>
                <a href="#" data-activates="slide-out" class="sidebar-collapse btn-floating btn-medium waves-effect waves-light hide-on-large-only darken-2"><i class="material-icons">menu</i></a>
            </aside>
            <!-- END LEFT SIDEBAR NAV-->

            <!-- //////////////////////////////////////////////////////////////////////////// -->

            <!-- START CONTENT -->
            <section id="content">

                <!--start container-->
                <div class="container">

                    <%= yield %>
                    


                </div>
                <!--end container-->
            </section>
            <!-- END CONTENT -->

            <!-- //////////////////////////////////////////////////////////////////////////// -->
           

        </div>
        <!-- END WRAPPER -->

    </div>
    <!-- END MAIN -->



    <!-- //////////////////////////////////////////////////////////////////////////// -->
    
     


    <!-- ================================================
    Scripts
    ================================================ -->
    
    
    <!--materialize js-->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/materialize.min.js"></script>
    <!--scrollbar-->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>
       

    <!-- chartist -->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/chartist-js/chartist.min.js"></script>   

    <!-- chartjs -->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/chartjs/chart.min.js"></script>
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/chartjs/chart-script.js"></script>

    <!-- sparkline -->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/sparkline/jquery.sparkline.min.js"></script>
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/sparkline/sparkline-script.js"></script>
    
    <!--jvectormap-->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins/jvectormap/vectormap-script.js"></script>
    
    
    <!--plugins.js - Some Specific JS codes for Plugin Settings-->
    <script type="text/javascript" src="http://www.yonkethecardepot.com/backup/assets/js/plugins.js"></script>
    <!-- Toast Notification -->
    <script type="text/javascript">
    
    <% unless notice.nil? %>
    // Toast Notification
    $(window).load(function() {
        setTimeout(function() {
            Materialize.toast('<span><%= notice %></span>', 1500);
        }, 3000);
        
    });
    <% end %>
    
    <% unless alert.nil? %>
    // Toast Notification
    $(window).load(function() {
        setTimeout(function() {
            Materialize.toast('<span><%= alert %></span>', 1500);
        }, 3000);
        
    });
    <% end %>
    
    </script>
</body>
</html>
